{
  "name": "q-types-module",
  "version": "1.0.5",
  "main": "dist/q-types-module.umd.js",
  "browser": "dist/q-types-module.browser.js",
  "module": "dist/q-types-module.esm-bundler.js",
  "unpkg": "dist/q-types-module.min.js",
  "repository": "https://github.com/a4anthony/q-types",
  "homepage": "https://github.com/a4anthony/q-types",
  "author": "Anthony Akro @a4anthony",
  "contributors": [
    {
      "name": "Anthony Akro",
      "url": "https://github.com/a4anthony"
    }
  ],
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-rollup": "rollup -c rollup.config.js && npm run tailwind",
    "build-bundle": "vue-cli-service build --target lib --name q-types-module ./src/wrapper.js",
    "build": "rm -rf dist && npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/q-types-module.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/q-types-module.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/q-types-module.min.js",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "tailwind-watch": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch  --minify",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --minify"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@storybook/addon-controls": "^6.4.9",
    "audio-recorder-polyfill": "^0.4.1",
    "autoprefixer": "^9.8.8",
    "core-js": "^3.6.5",
    "lodash": "^4.17.21",
    "postcss": "^7.0.39",
    "prettier": "^2.5.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "tailwind-safelist-generator": "^0.1.3",
    "tailwind-scrollbar": "^1.3.1",
    "vue": "^3.0.0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-viewport": "^6.4.9",
    "@storybook/vue3": "^6.4.9",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "rollup": "^2.43.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "vue-inline-svgs": "^1.0.7",
    "vue-loader": "^16.8.3"
  }
}
